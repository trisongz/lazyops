{
    "$schema": "https://mintlify.com/schema.json",
    "name": "{{ source.app_name }} Docs",
    "logo": {
      "dark": "/logos/wordmark-dark.svg",
      "light": "/logos/wordmark.svg",
      "href": "{{ source.app_endpoint }}"
    },
    "favicon": "/logos/favicon.png",
    "colors": {
      "primary": "#9563FF",
      "light": "#AE87FF",
      "dark": "#0D001D",
      "background": {
        "dark": "#090014"
      },
      "anchors": {
        "from": "#FF7F57",
        "to": "#9563FF"
      }
    },
    "backgroundImage": "/images/background.png",
    "topbarLinks": [
      {
        "name": "Support",
        "url": "mailto:support@dub.co"
      }
    ],
    "topbarCtaButton": {
      "name": "Dashboard",
      "url": "https://app.dub.co"
    },
    "openapi": "http://api.dub.co",
    "tabs": [
      {
        "name": "API Reference",
        "url": "api-reference"
      },
      {
        "name": "SDKs",
        "url": "sdks"
      }
    ],
    "anchors": [
      {
        "name": "Help Center",
        "icon": "circle-info",
        "iconType": "sharp-solid",
        "url": "https://dub.co/help"
      },
      {
        "name": "GitHub",
        "icon": "github",
        "url": "https://github.com/dubinc/dub"
      }
    ],
    "navigation": [
      {
        "group": "Getting Started",
        "pages": ["introduction", "data-model"]
      },
      {
        "group": "Quickstart",
        "pages": [
          {
            "group": "Typescript",
            "icon": "js",
            "pages": [
              "quickstart/typescript",
              "quickstart/nextjs",
              "quickstart/remix",
              "quickstart/nuxt",
              "quickstart/express"
            ]
          },
          {
            "group": "Go",
            "icon": "golang",
            "pages": ["quickstart/go"]
          },
          {
            "group": "Python",
            "icon": "python",
            "pages": [
              "quickstart/python",
              "quickstart/flask",
              "quickstart/django"
            ]
          },
          {
            "group": "Ruby",
            "icon": "gem",
            "pages": ["quickstart/ruby"]
          }
        ]
      },
      {
        "group": "Resources",
        "pages": ["integrations", "examples"]
      },
      {
        "group": "Conversions (BETA)",
        "pages": [
          "conversions/quickstart",
          {
            "group": "Leads",
            "icon": "crosshairs",
            "pages": [
              "conversions/leads/introduction",
              "conversions/leads/next-auth",
              "conversions/leads/clerk",
              "conversions/leads/supabase-auth",
              "conversions/leads/auth0"
            ]
          },
          {
            "group": "Sales",
            "icon": "money-bill",
            "pages": [
              "conversions/sales/introduction",
              "conversions/sales/stripe",
              "conversions/sales/shopify"
            ]
          }
        ]
      },
      {
        "group": "Open source",
        "pages": ["local-development", "self-hosting"]
      },
      {
        "group": "API Reference",
        "pages": [
          "api-reference/introduction",
          "api-reference/tokens",
          "api-reference/rate-limits",
          "api-reference/errors"
        ]
      },
      {
        "group": "Links",
        "pages": [
          "api-reference/endpoint/create-a-new-link",
          "api-reference/endpoint/retrieve-a-link",
          "api-reference/endpoint/update-a-link",
          "api-reference/endpoint/upsert-a-link",
          "api-reference/endpoint/delete-a-link",
          "api-reference/endpoint/retrieve-a-list-of-links",
          "api-reference/endpoint/retrieve-number-of-links",
          "api-reference/endpoint/bulk-create-links",
          "api-reference/endpoint/bulk-update-links"
        ]
      },
      {
        "group": "Analytics",
        "pages": ["api-reference/endpoint/retrieve-analytics"]
      },
      {
        "group": "QR Codes",
        "pages": ["api-reference/endpoint/retrieve-a-qr-code"]
      },
      {
        "group": "Workspaces",
        "pages": ["api-reference/endpoint/retrieve-a-workspace"]
      },
      {
        "group": "Domains",
        "pages": [
          "api-reference/endpoint/create-a-domain",
          "api-reference/endpoint/update-a-domain",
          "api-reference/endpoint/delete-a-domain",
          "api-reference/endpoint/retrieve-a-list-of-domains"
        ]
      },
      {
        "group": "Tags",
        "pages": [
          "api-reference/endpoint/create-a-new-tag",
          "api-reference/endpoint/retrieve-a-list-of-tags"
        ]
      },
      {
        "group": "Metatags",
        "pages": ["api-reference/endpoint/retrieve-metatags"]
      },
      {
        "group": "Conversions (BETA)",
        "pages": [
          "api-reference/endpoint/track-lead",
          "api-reference/endpoint/track-sale",
          "api-reference/endpoint/track-customer"
        ]
      },
      {
        "group": "SDKs",
        "pages": ["sdks/overview", "sdks/js-client/overview"]
      },
      {
        "group": "TypeScript",
        "pages": [
          "sdks/typescript/overview",
          {
            "group": "Links",
            "icon": "link",
            "pages": [
              "sdks/typescript/links/create",
              "sdks/typescript/links/get",
              "sdks/typescript/links/update",
              "sdks/typescript/links/upsert",
              "sdks/typescript/links/delete",
              "sdks/typescript/links/list",
              "sdks/typescript/links/count",
              "sdks/typescript/links/create-many"
            ]
          },
          {
            "group": "Analytics",
            "icon": "chart-line",
            "pages": ["sdks/typescript/analytics/retrieve"]
          },
          {
            "group": "Workspaces",
            "icon": "folder",
            "pages": [
              "sdks/typescript/workspaces/create",
              "sdks/typescript/workspaces/get",
              "sdks/typescript/workspaces/list"
            ]
          },
          {
            "group": "Domains",
            "icon": "globe",
            "pages": [
              "sdks/typescript/domains/create",
              "sdks/typescript/domains/update",
              "sdks/typescript/domains/delete",
              "sdks/typescript/domains/list"
            ]
          },
          {
            "group": "Tags",
            "icon": "tag",
            "pages": ["sdks/typescript/tags/create", "sdks/typescript/tags/list"]
          },
          {
            "group": "Metatags",
            "icon": "image",
            "pages": ["sdks/typescript/metatags/get"]
          }
        ]
      }
    ],
    "api": {
      "maintainOrder": true
    },
    "feedback": {
      "thumbsRating": true,
      "suggestEdit": true,
      "raiseIssue": true
    },
    "footerSocials": {
      "twitter": "https://twitter.com/dubdotco",
      "github": "https://github.com/dubinc/dub",
      "linkedin": "https://www.linkedin.com/company/dubinc",
      "website": "https://dub.co"
    },
    "redirects": [
      {
        "source": "/sdks",
        "destination": "/sdks/overview"
      },
      {
        "source": "/integrations/:path*",
        "destination": "/integrations"
      },
